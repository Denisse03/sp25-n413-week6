{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{ "color": "#f0f0f4", "name": "Background", "slug": "background" },
				{
					"color": "#4d361b",
					"name": "Primary",
					"slug": "primary"
				},
				{ "color": "#cda75f", "name": "Secondary", "slug": "secondary" }
			]
		}
	},
	"styles": {
		"color": {
			"background": "var:preset|color|background",
			"text": "var:preset|color|text"
		},
		"elements": {
			"h1": {
				"color": {
					"text": " #4d361b"
				}
			},

			"h3": {
				"spacing": {
					"padding": {
						"left": "300px"
					}
				},
				"color": {
					"text": " #4d361b"
				}
			},
			"button": {
				"color": {
					"background": "#4d361b"
				},
				"spacing": {
					"margin": {
						"left": "700px"
					}
				}
			},
			"h4": {
				"spacing": {
					"padding": {
						"left": "300px"
					}
				},
				"color": {
					"text": " #4d361b"
				}
			},
			"h5": {
				"spacing": {
					"padding": {
						"left": "300px"
					}
				},
				"color": {
					"text": " #4d361b"
				}
			}
		},
		"blocks": {
			"core/paragraph": {
				"spacing": {
					"padding": {
						"left": "300px"
					}
				}
			},
			"core/list": {
				"spacing": {
					"padding": {
						"left": "350px"
					}
				},
				"color": {
					"text": " #4d361b"
				}
			}
		}
	}
}
